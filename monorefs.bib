	month = dec,
	year = {2016},
	keywords = {complex networks, stability analysis, definiteness of matrices},
	pages = {888--903},
	file = {Full Text PDF:/home/olie/Zotero/storage/E78T9W6U/Do et al. - 2016 - Topological stability criteria for networking dyna.pdf:application/pdf;Snapshot:/home/olie/Zotero/storage/TH3TNAWW/5FB5088DC58DC6823E0FC1D5C8E5F03C.html:text/html}
}

