	pages = {137},
	file = {Full Text PDF:/home/olie/Zotero/storage/D7EDX4XB/Kitano - 2007 - Towards a theory of biological robustness.pdf:application/pdf;Snapshot:/home/olie/Zotero/storage/URJWU74G/137.html:text/html}
}

