  keywords  = {Graph theory, graph drawing, Clustering algorithms, Force, Layout, layout algorithm, C++ languages, Clustered graph, clustered graph drawing algorithms, computational complexity, edge crossings, Electrical engineering, Electronic mail, force-directed placement, force-directed placement algorithm, heuristics, intracluster graph nodes, outside connectivity, pattern clustering, running time, spring-electrical force model, Stress, stress majorization, stress model, torque, torque equilibrium},
  timestamp = {2017-05-24T11:20:11Z},
