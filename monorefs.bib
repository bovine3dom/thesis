  groups     = {Interesting,interesting enough to read,Interesting,interesting enough to read,Visualisation},
  keywords   = {Categories and Subject Descriptors (according to {ACM} CCS): H.5.2 [Information Interfaces and Presentation]: User Interfaces—Graphical user interfaces (GUI), graph visualization, group visualization, Literature survey},
  language   = {en},
  shorttitle = {Visualizing {Group} {Structures} in {Graphs}},
  timestamp  = {2017-05-24T11:24:51Z},
  url        = {http://onlinelibrary.wiley.com/doi/10.1111/cgf.12872/abstract},
  urldate    = {2017-05-24},
}

@InProceedings{bhunre_solving_2016,
  author    = {Bhunre, Piyush K. and Bhowmick, Partha and Mukhopadhyay, Jayanta},
  title     = {Solving {Distance} {Geometry} {Problem} with {Inexact} {Distances} in {Integer} {Plane}},
