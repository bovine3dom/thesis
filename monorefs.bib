	keywords = {complex networks},
	pages = {65--68},
	file = {Full Text PDF:/home/olie/Zotero/storage/ZCC92JGD/Morone and Makse - 2015 - Influence maximization in complex networks through.pdf:application/pdf;Snapshot:/home/olie/Zotero/storage/ZE493J5I/nature14604.html:text/html}
}

