@Article{He2014a,
  author    = {He, Jun-Zhou and Wang, Rui-Wu and Li, Yao-Tang},
  title     = {Evolutionary {Stability} in the {Asymmetric} {Volunteer}'s {Dilemma}},
  journal   = {PLOS ONE},
  year      = {2014},
  volume    = {9},
  number    = {8},
  pages     = {e103931},
  month     = aug,
  issn      = {1932-6203},
  abstract  = {It is often assumed that in public goods games, contributors are either strong or weak players and each individual has an equal probability of exhibiting cooperation. It is difficult to explain why the public good is produced by strong individuals in some cooperation systems, and by weak individuals in others. Viewing the asymmetric volunteer's dilemma game as an evolutionary game, we find that whether the strong or the weak players produce the public good depends on the initial condition (i.e., phenotype or initial strategy of individuals). These different evolutionarily stable strategies (ESS) associated with different initial conditions, can be interpreted as the production modes of public goods of different cooperation systems. A further analysis revealed that the strong player adopts a pure strategy but mixed strategies for the weak players to produce the public good, and that the probability of volunteering by weak players decreases with increasing group size or decreasing cost-benefit ratio. Our model shows that the defection probability of a “strong” player is greater than the “weak” players in the model of Diekmann (1993). This contradicts Selten's (1980) model that public goods can only be produced by a strong player, is not an evolutionarily stable strategy, and will therefore disappear over evolutionary time. Our public good model with ESS has thus extended previous interpretations that the public good can only be produced by strong players in an asymmetric game.},
  doi       = {10.1371/journal.pone.0103931},
  file      = {Full Text PDF:/home/olie/.mozilla/firefox/xcxckdau.default/zotero/storage/XPKR9CIB/He et al. - 2014 - Evolutionary Stability in the Asymmetric Volunteer.pdf:application/pdf;Snapshot:/home/olie/.mozilla/firefox/xcxckdau.default/zotero/storage/VRGB9MWA/article.html:text/html},
  groups    = {Forward,Forward,Forward,Interesting,Extra useful,Interesting,Extra useful},
  keywords  = {Chemical equilibrium, Evolutionary algorithms, Evolutionary theory, Games, Game theory, Nonlinear systems, Police, System stability},
  timestamp = {2016-11-07T12:05:56Z},
  url       = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0103931},
  urldate   = {2016-11-07},
}

