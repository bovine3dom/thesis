	keywords = {a, B, c, d, e, g, h, i, k, l, m, n, o, p, r, S, t, u, v, w, y},
	pages = {e1004055},
	file = {Full Text PDF:/home/olie/Zotero/storage/N9K5NGE8/Friedlander et al. - 2015 - Evolution of Bow-Tie Architectures in Biology.pdf:application/pdf;Snapshot:/home/olie/Zotero/storage/HBTGMF22/article.html:text/html}
