@Article{Hirota2011a,
  author    = {Hirota, Marina and Holmgren, Milena and Nes, Egbert H. Van and Scheffer, Marten},
  title     = {Global {Resilience} of {Tropical} {Forest} and {Savanna} to {Critical} {Transitions}},
