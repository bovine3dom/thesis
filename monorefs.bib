  file      = {APS Snapshot:/home/olie/.mozilla/firefox/xcxckdau.default/zotero/storage/N6TZKJZG/PhysRevLett.86.html:text/html;Full Text PDF:/home/olie/.mozilla/firefox/xcxckdau.default/zotero/storage/8RA4C892/Pastor-Satorras and Vespignani - 2001 - Epidemic Spreading in Scale-Free Networks.pdf:application/pdf},
