	author = {Steglich, Christian and Sinclair, Philip and Holliday, Jo and Moore, Laurence},
	month = jul,
	year = {2012},
	keywords = {Adolescent friendship, Peer influence, Smoking uptake, Stochastic actor-based modeling},
	pages = {359--369},
	file = {ScienceDirect Full Text PDF:/home/olie/Zotero/storage/ZF8W3PBQ/Steglich et al. - 2012 - Actor-based analysis of peer influence in A Stop S.pdf:application/pdf;ScienceDirect Snapshot:/home/olie/Zotero/storage/48IGDT6W/S0378873310000365.html:text/html}
