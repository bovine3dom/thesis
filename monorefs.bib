@article{agneessens_where_2012,
	series = {Dynamics of {Social} {Networks} (2)},
	title = {Where do intra-organizational advice relations come from? {The} role of informal status and social capital in social exchange},
	volume = {34},
	issn = {0378-8733},
	shorttitle = {Where do intra-organizational advice relations come from?},
	url = {http://www.sciencedirect.com/science/article/pii/S0378873311000189},
	doi = {10.1016/j.socnet.2011.04.002},
	abstract = {Social status and social capital frameworks are used to derive competing hypotheses about the emergence and structure of advice relations in organizations. Although both approaches build on a social exchange framework, they differ in their behavioral micro-foundations. From a status perspective, advice giving is a means to generate prestige, whereas asking advice decreases one's relative standing. At a structural level these motivations are expected to result in an overrepresentation of non-reciprocal dyads and non-cyclical triadic structures in the advice network, as well as in active advice seekers being unlikely to be approached for advice, especially by active advice givers. From a social capital perspective, advice seeking creates obligations for the advice seeker. At the structural level, this results in an overrepresentation of reciprocal dyads and cyclical triads, and active advice seekers to be unpopular as targets of advice seeking, especially for active advice givers. Analyses of four waves of a longitudinal sociometric study of 57 employees of a Dutch Housing Corporation provide partial support for both approaches. In line with the social capital perspective, we find reciprocal advice relations to be overrepresented at the dyad level. Results at the triad level support the social status arguments, according to which high status individuals will avoid asking advice from low status individuals. The implications for macro-structural properties of intra-organizational advice network are discussed.},
	number = {3},
	urldate = {2017-03-21},
	journal = {Social Networks},
	author = {Agneessens, Filip and Wittek, Rafael},
	month = jul,
	year = {2012},
	keywords = {social network analysis, Advice relation, Cycle, Longitudinal analysis, Organizations, Social capital, Social exchange theory, Status, Triads},
	pages = {333--345},
	file = {ScienceDirect Full Text PDF:/home/olie/Zotero/storage/J3VBE3UF/Agneessens and Wittek - 2012 - Where do intra-organizational advice relations com.pdf:application/pdf;ScienceDirect Full Text PDF:/home/olie/Zotero/storage/BPKADXMQ/Agneessens and Wittek - 2012 - Where do intra-organizational advice relations com.pdf:application/pdf;ScienceDirect Snapshot:/home/olie/Zotero/storage/FWDES9M5/S0378873311000189.html:text/html;ScienceDirect Snapshot:/home/olie/Zotero/storage/6STPPKGC/S0378873311000189.html:text/html}
