  keywords = {cluster analysis,complex networks,language classification,language network,linguistic typology,syntactic dependency network},
}

@Article{Pei2014,
  author   = {Pei, Wenzhe and Ge, Tao and Chang, Baobao},
  title    = {{Max-margin tensor neural network for Chinese word segmentation}},
