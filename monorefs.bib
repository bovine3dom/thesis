  groups    = {Gao \& Barzel,Gao \& Barzel,Interesting,interesting enough to read,favourites,Interesting,interesting enough to read,favourites},
  language  = {en},
  pmid      = {23087241},
  timestamp = {2016-11-07T11:10:32Z},
  url       = {http://science.sciencemag.org/content/338/6105/344},
  urldate   = {2016-11-07},
}

