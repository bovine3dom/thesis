  keywords = {Graphs, Algorithms, Hypertext, Information theory, Network analysis, Optimization, Scale-free networks, Social networks},
  url      = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0080783},
  urldate  = {2016-08-22},
}

