  pmid       = {27014079},
  shorttitle = {Predicting {Essential} {Genes} and {Proteins} {Based} on {Machine} {Learning} and {Network} {Topological} {Features}},
  url        = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4781880/},
  urldate    = {2017-07-10},
}

