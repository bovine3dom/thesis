@Article{rameseder_multivariate_2015,
  author    = {Rameseder, Jonathan and Krismer, Konstantin and Dayma, Yogesh and Ehrenberger, Tobias and Hwang, Mun Kyung and Airoldi, Edoardo M. and Floyd, Scott R. and Yaffe, Michael B.},
  title     = {A {Multivariate} {Computational} {Method} to {Analyze} {High}-{Content} {RNAi} {Screening} {Data}},
  journal   = {Journal of Biomolecular Screening},
  year      = {2015},
  volume    = {20},
