@Article{louzada_smart_2013,
  author   = {Louzada, V. H. P. and Daolio, F. and Herrmann, H. J. and Tomassini, M.},
  title    = {Smart rewiring for network robustness},
  journal  = {Journal of Complex Networks},
  year     = {2013},
