  keywords  = {British and Irish Literature, History of Science, History of Technology, Literary Theory, Media Studies, Modern History},
  language  = {en},
  timestamp = {2017-05-24T11:20:45Z},
  url       = {http://link.springer.com/chapter/10.1057/9781137393296_7},
  urldate   = {2017-05-24},
}

