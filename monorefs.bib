}

@Article{Scheffer2012a,
  author    = {Scheffer, Marten and Carpenter, Stephen R. and Lenton, Timothy M. and Bascompte, Jordi and Brock, William and Dakos, Vasilis and Koppel, Johan van de and Leemput, Ingrid A. van de and Levin, Simon A. and Nes, Egbert H. van and Pascual, Mercedes and Vandermeer, John},
